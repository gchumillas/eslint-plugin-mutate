{
  "name": "eslint-plugin-mutate",
  "version": "1.0.0",
  "description": "ESLint plugin to enforce mutation awareness by requiring 'mut' prefix for mutable parameters",
  "main": "index.js",
  "keywords": [
    "eslint",
    "plugin",
    "mutation",
    "immutable",
    "javascript"
  ],
  "author": "Gonzalo Chumillas",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "mocha": "^10.0.0"
  },
  "scripts": {
    "test": "mocha tests/**/*.test.js",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "test:rule": "mocha tests/rules/require-mut-prefix.test.js",
    "test:plugin": "mocha tests/plugin.test.js",
    "test:edge": "mocha tests/rules/edge-cases.test.js",
    "test:single": "mocha"
  },
  "engines": {
    "node": ">=12.0.0"
