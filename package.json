{
  "name": "eslint-plugin-mutate",
  "version": "1.1.0",
  "description": "ESLint plugin to enforce mutation awareness by requiring 'mut' prefix for mutable parameters",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "eslint",
    "plugin",
    "mutation",
    "immutable",
    "javascript",
    "typescript",
    "functional-programming",
    "side-effects",
    "linting"
  ],
  "author": "Gonzalo Chumillas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gchumillas/eslint-plugin-mutate.git"
  },
  "bugs": {
    "url": "https://github.com/gchumillas/eslint-plugin-mutate/issues"
  },
  "homepage": "https://github.com/gchumillas/eslint-plugin-mutate#readme",
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "mocha": "^10.0.0"
  },
  "scripts": {
    "test": "mocha tests/**/*.test.js",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "test:param": "mocha tests/rules/require-mut-param-prefix.test.js",
    "test:param-ts": "mocha tests/rules/require-mut-param-prefix-typescript.test.js",
    "test:var": "mocha tests/rules/require-mut-var-prefix.test.js",
    "test:plugin": "mocha tests/plugin.test.js",
    "test:edge": "mocha tests/rules/edge-cases.test.js",
    "test:single": "mocha"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
